import java.awt.List;
//import java.util.*;




public class Solution {

	public static double findMaxAverage(int[] nums, int k) {

		int local = 0;
		int global = 0;

		for(int i=0;i<k;i++){
			local=local+nums[i];
		}
		global=local;
		
		for (int i = 0; i < nums.length-k; i++) {
			local = local+nums[i+k]-nums[i];
			global = Math.max(global, local);
		}
		return (double)global/k;
	}
	


	public static void main(String[] args) {
		
		double s= 2.5+5+5.5+106.49-27-27-27+71+6.5+9.5+9+12.6+10+7.5+13.9+15+10+13+11.8+13+9+10.66+25+11+12+14.8+13+10+10+14.3+8+9.5+11.5+6.5+8.6+12.6+6.98+38+10+12.5+1.5+100+26.1+8.4+6.7+7+4.6+15+4+11.8+13.1;
		
		System.out.println(s);
		//System.out.println(s1);
		//System.out.println(s2);
		//System.out.println(s+s1+s2);
	}
}